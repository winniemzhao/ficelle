<% @inspos.each do |inspo| %>
  <%= inspo.photo.attached? ? cl_image_tag(inspo.photo.key, width: 400, height: 300, crop: :fill) : cl_image_tag("wtu1z51wksknvgrbgbie", width: 400, height: 300, crop: :fill) %>
  <%= inspo.name %>
  <%= inspo.genre %>
  <%= link_to "like", toggle_favorite_inspo_path(inspo),  data: {turbo_method: :post}, class:'btn-flat' %>
<% end %>

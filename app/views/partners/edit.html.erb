<%= simple_form_for [@partner] do |f| %>
<%= f.input :name %>
<%= f.input :email %>
<%= f.input :location %>
<%= f.input :birthday %>
<%= f.input :phone_number %>
<%= f.input :photo, as: :file, hint: "Upload new photo" %>
<%= f.submit "Save Changes", class: "btn btn-primary" %>
<% end %>

